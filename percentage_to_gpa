def per_to_gpa(yourmarks):
    increasedmarks = float(yourmarks)/25
    return increasedmarks

yourmarks = float(input("Type Your Percentage Here - "))

if yourmarks > 100:
    print("Even Einstein Can't Score That Much")
elif yourmarks < 0:
    print("Don't act smart buddy...")
elif 80 < yourmarks < 100:
    print("Conragulations on Getting a GPA of", round(per_to_gpa(yourmarks), 1))
elif 0 < yourmarks < 33:
    print("Sorry, But Your GPA is -", round(per_to_gpa(yourmarks), 1))
else:
    print("Your GPA is -", round(per_to_gpa(yourmarks), 1))
